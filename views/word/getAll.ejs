<header class="">
  <h1 class="display-10"><%=title%></h1>
</header>
<hr style="margin: 5px; ">
  <% words.forEach( (w) => { %>
    <div class="media">
      <svg class="bd-placeholder-img mr-3" width="32" height="64" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: 64x64"><title>Placeholder</title><rect width="100%" height="100%" fill="#868e96"/><text x="50%" y="50%" fill="#dee2e6" dy=".3em"><%= w.keyword %></text></svg>
      <div class="media-body">
        <h5 class="mt-0"><%= w.keyword %></h5>
        <p class="max_300px .w-80"><%= w.discription %></p>
      </div>
    </div>
    <hr style="margin: 5px; ">
  <% }); %>

